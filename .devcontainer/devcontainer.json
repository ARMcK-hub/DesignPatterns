{
	"name": "DesignPatterns",
	"dockerComposeFile": [
		"../docker-compose.yml"
	],
	"service": "dev-container",
	"workspaceFolder": "/workspaces/DesignPatterns",
	"postCreateCommand": "cd $WORKSPACE && poetry install",
	"settings": {
		"terminal.integrated.profiles.linux": {
			"bash": {
				"path": "bash",
				"icon": "terminal-bash"
			},
			"pwsh": {
				"path": "pwsh",
				"icon": "terminal-powershell"
			}
		}
	},
	"extensions": [
		"ms-python.python",
		"ms-python.vscode-pylance"
	]
}